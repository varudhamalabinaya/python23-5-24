arr1 = list(map(int, input("Enter the elements of the first array: ").split()))
arr2 = list(map(int, input("Enter the elements of the second array: ").split()))
max_element_arr1 = max(arr1)
min_element_arr2 = min(arr2)
product = max_element_arr1 * min_element_arr2
print("Product of max element of first array and min element of second array:", product)
